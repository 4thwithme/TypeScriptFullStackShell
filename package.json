{
  "name": "typescriptfullstackshell",
  "version": "1.0.0",
  "description": "demonstrate how to do full-stack TypeScript development",
  "main": "start.js",
  "scripts": {
    "start": "npm install --only=prod && node start.js prod",
    "start:express": "tsc-watch --onSuccess \"node start.js dev\" --onFailure \"echo Beep! Compilation Failed\" ",
    "start:react": "cd ./src/public/react/demo-react/ && npm start",
    "test": "tsc && node start.js test",
    "build": "sh ./util/buildForProd.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpmaxwell/TypeScriptFullStackShell.git"
  },
  "keywords": [
    "TypeScript",
    "Express",
    "React",
    "FullStack",
    "Professional",
    "Enterprise",
    "Startup"
  ],
  "author": "Sean Maxwell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanpmaxwell/TypeScriptFullStackShell/issues"
  },
  "homepage": "https://github.com/seanpmaxwell/TypeScriptFullStackShell#readme",
  "dependencies": {
    "@overnightjs/core": "^1.3.3",
    "express": "^4.16.4",
    "simple-color-print": "^1.0.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^3.3.7",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jasmine": "^3.3.1",
    "tsc": "^1.20150623.0",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
